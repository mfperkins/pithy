.carousel#welcome-carousel.carousel-slider.center{"data-indicators" => "true"}
  .carousel-fixed-item.center
    .col.s6
      %a{:href => new_api_v1_slack_auth_path}
        %img{:alt => "Add to Slack", :height => "40", :src => "https://platform.slack-edge.com/img/add_to_slack.png", :srcset => "https://platform.slack-edge.com/img/add_to_slack.png 1x, https://platform.slack-edge.com/img/add_to_slack@2x.png 2x", :width => "139"}
  .carousel-item#home-img.black-text{:href => "#one!"}
    = render 'home/welcome'
  .carousel-item.black-text{:href => "#two!"}
    = render 'home/get_started'
